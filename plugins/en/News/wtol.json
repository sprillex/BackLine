{
  "domainPattern": ".*wtol\\.com",
  "strategy": "CSS_SELECTOR",
  "targetIdentifier": "ignored",
  "contentPath": "div.article__body",
  "titlePath": "h1.article__headline",
  "injectRssImage": true,
  "sourceName": "WTOL",
  "removeSelectors": [
    "iframe",
    "script",
    ".video-container",
    ".embed-container",
    ".iframe-wrapper",
    ".video-player",
    ".jwplayer-container"
  ]
}
